<h1>Listing Products</h1>

<table>
  <thead>
    <tr>
      <th>Gsname</th>
      <th>Gsgoodssize</th>
      <th>Gsgoodssize</th>
      <th>Gsbestbeforestart</th>
      <th>Gs bestbeforend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.gsname %></td>
        <td><%= product.gsgoodsSize %></td>
        <td><%= product.gsgoodsSize %></td>
        <td><%= product.gsBestbeforeStart %></td>
        <td><%= product.gs_BestbeforEnd %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

