<!-- コンテナ開始 -->

　
    <div class="col-md-5">
<div class="panel panel-success">
　<div class="panel-heading">物資在庫登録</div>
    <div class="panel-body">
      <div class="row">
        <div class="col-sm-3">物資名</div>
        <div class="col-sm-9 form-inline" style="padding: 3px;">
          <input type="text" class="form-control input-sm" id="name" placeholder="コーラ" size="50">
        </div>
      </div>
      <div class="row">
        <div class="col-sm-3">在庫所持数</div>
        <div class="col-sm-9 form-inline" style="padding: 3px;">
          <input type="text" class="form-control input-sm" id="name" placeholder="200" size="10">
        </div>
      </div>
      <div class="row">
       <div class="col-sm-3">不足数</div>
        <div class="col-sm-9 form-inline" style="padding: 3px;">
          <input type="text" class="form-control input-sm" id="name" placeholder="150" size="10">
        </div>
      </div>
      <div class="row">
       <div class="col-sm-3">不要数</div>
        <div class="col-sm-9 form-inline" style="padding: 3px;">
          <input type="text" class="form-control input-sm" id="name" placeholder="150" size="10">
        </div>
      </div>
     　<div class="row">
       <div class="col-sm-3">備考</div>
        <div class="col-sm-9 form-inline" style="padding: 3px;">
          <textarea name="example1" cols="45" rows="5"></textarea>
       </div>
      </div>
      <div class="text-center" style="padding: 30px;">
        <button type="button" class="btn btn-success">入力内容仮登録 </button>
        <button type="button" class="btn btn-success">削除</button>
      　<button type="button" class="btn btn-success">新規写真登録 </button>
      </div>
    </div>
   </div>
</div>


  　 <table class="table table-bordered">
	<tbody>
	    <th>品名</th>
	    <td colspan="12"><%= f.text_field :gs_name, class: "form-control input-sm", placeholder: "コーラ" %></td>
		</tr>
		<tr>
			<th colspan="2">サイズ</th>
			<th colspan="3">段ボール</th>
			<th colspan="2">賞味期限</th>
			<th colspan="3">物資詳細</th>
		</tr>
		<tr>
			<th>種目</th>
			<th>大きさcm</th>
			<th>重量ｇ</th>
			<th>１箱の商品入数</th>
			<th>サイズ<br>縦×横×幅cm</th>
			<th>開始日時</th>
			<th>終了日時</th>
			<th>在庫数</th>
			<th>不足数</th>
			<th>在庫数</th>
		</tr>
		<tr>
		
			<td><%= f.select :gs_sizeEvent, {"なし" => "なし" , "大人" => "大人" ,"大人男" => "大人男" , "大人女" => "大人女"  , "子供女" => "子供女","子供男" => "子供男"} %></td>
			<td><%= f.text_field :gs_goodsSize, class: "form-control input-sm", placeholder: "S" %></td>
			<td><%= f.text_field :gs_dbweight, class: "form-control input-sm", placeholder: "10" %></td>
			<td><%= f.text_field :gs_dbboxcount, class: "form-control input-sm", placeholder: "10" %></td>
			<td><%= f.text_field:gs_dbboxsize, class: "form-control input-sm", placeholder: "10×20×30" %></td>
	    	<td><%= f.text_field:gs_BestbeforeStart, class: "form-control input-sm", placeholder: "2017/02/09㈮" %></td>
			<td><%= f.text_field:gs_BestbeforEnd, class: "form-control input-sm", placeholder: "2017/02/10㈯" %></td>
			<td><%= f.text_field:gs_NowNum, class: "form-control input-sm", placeholder: "30" %></td>
			<td><%= f.text_field:gs_shortages, class: "form-control input-sm", placeholder: "30" %></td>
			<td><input type="text" class="form-control input-sm" id="name" placeholder="200" size="10"></td>
		</tr>
		<tr>
		<th>備考</th>
	    <td colspan="12"> <%= text_area :goos_note, :comtent, :size => "70x5" %></textarea></td>
		</tr>
		<tr>    
	</tbody>
</table>

           