<div  class="container">
    <div class="example">
     <div class="panel panel-primary">  
      <div class="panel-heading">持っているもの多い順のアイテム上位10個</div>
      <table class="table table-striped table-bordered">
          <caption><a href="http://umemurayuka-monolist2.herokuapp.com">HerokuのURL</a></caption>
         <tbody>
    		<tr class="danger">
    			<th>順位</th>
    			<th>商品名</th>
    			<th>備考</th>
    		</tr>
    		<tr><% @items.each.with_index(1) do |item,i| %>
    			<th><%= "#{i}" %></th>
    			<td><%= image_tag(item.large_image , alt: item.title) %></td>
    			<td><%= item.title %></td>
    		</tr>
    		<% end %>
    	</tbody>
      </table>
      </div>
　</div>
</div>